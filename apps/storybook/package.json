{
  "name": "@apps/storybook",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "code d'azur",
    "email": "development@codedazur.com",
    "url": "https://codedazur.com"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7.14",
    "yarn": "Please use NPM!"
  },
  "scripts": {
    "develop": "storybook dev -c . -p 6006 --no-open",
    "audit": "npm audit --omit dev --audit-level moderate",
    "audit:fix": "npm audit fix --omit dev",
    "lint": "eslint .",
    "style": "prettier --check .",
    "types": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "format": "prettier --write .",
    "build": "storybook build -c . -o .storybook"
  },
  "dependencies": {
    "@codedazur/fusion-ui": "^1.3.1",
    "@faker-js/faker": "^9.9.0",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.17",
    "@storybook/nextjs": "^9.0.17",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/recipes": "^0.5.7",
    "storybook": "^9.0.17"
  },
  "devDependencies": {
    "@codedazur/tsconfig": "^1.0.0",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/react-webpack5": "^9.0.18",
    "@storybook/test-runner": "^0.23.0",
    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.17",
    "mini-css-extract-plugin": "^2.9.2",
    "style-loader": "^4.0.0"
  }
}
