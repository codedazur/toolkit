{
  "name": "@apps/storybook",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "code d'azur",
    "email": "development@codedazur.com",
    "url": "https://codedazur.com"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7.14",
    "yarn": "Please use NPM!"
  },
  "scripts": {
    "develop": "storybook dev -c . -p 6006 --no-open",
    "audit": "npm audit --omit dev --audit-level high",
    "audit:fix": "npm audit fix --omit dev",
    "lint": "eslint .",
    "style": "prettier --check .",
    "types": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch --passWithNoTests",
    "format": "prettier --write .",
    "build": "storybook build -c . -o .storybook"
  },
  "dependencies": {
    "@codedazur/essentials": "*",
    "@codedazur/fusion-ui": "^1.3.1",
    "@codedazur/react-date-picker": "*",
    "@codedazur/react-dictionary": "*",
    "@codedazur/react-essentials": "*",
    "@codedazur/react-forms": "*",
    "@codedazur/react-media": "*",
    "@codedazur/react-notifications": "*",
    "@codedazur/react-pagination": "*",
    "@codedazur/react-parallax": "*",
    "@codedazur/react-preferences": "*",
    "@codedazur/react-select": "*",
    "@codedazur/react-tracking": "*",
    "@faker-js/faker": "^10.2.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/nextjs": "^10.1.11",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/recipes": "^0.5.7",
    "storybook": "^10.1.11"
  },
  "devDependencies": {
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/react-webpack5": "^10.1.11",
    "@storybook/test-runner": "^0.24.2",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "@vanilla-extract/webpack-plugin": "^2.3.25",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "style-loader": "^4.0.0"
  }
}
