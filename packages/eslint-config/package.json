{
  "name": "@codedazur/eslint-config",
  "version": "1.0.2",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cdk": {
      "types": "./dist/cdk.d.ts",
      "default": "./dist/cdk.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "default": "./dist/next.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    },
    "./sanity": {
      "types": "./dist/sanity.d.ts",
      "default": "./dist/sanity.js"
    },
    "./storybook": {
      "types": "./dist/storybook.d.ts",
      "default": "./dist/storybook.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "develop": "tsup --watch",
    "build": "tsup",
    "audit": "npm audit --omit dev --audit-level high",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@sanity/eslint-config-studio": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-awscdk": "^4.0.5",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-turbo": "^2.7.4"
  }
}
